{
  "name": "sofinance",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "webpack serve --config web/webpack.config.js",
    "web:build": "webpack --config web/webpack.config.js --mode production",
    "web:dev": "webpack serve --config web/webpack.config.js --mode development",
    "dev:all": "concurrently \"npm run start\" \"npm run web\"",
    "dev:mobile": "concurrently \"npm run android\" \"npm run ios\"",
    "dev:web-mobile": "concurrently \"npm run web\" \"npm run start\"",
    "dev:multi": "node scripts/dev-multi.js",
    "dev:both": "node scripts/dev-all.js",
    "build:web": "webpack --config web/webpack.config.js --mode production",
    "build:mobile": "expo build",
    "test:web": "npm run web",
    "test:mobile": "npm run start",
    "clean": "expo start --clear && npm run web -- --config web/webpack.config.js --mode development"
  },
  "dependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "expo": "~52.0.0",
    "expo-status-bar": "~2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "lucide-react": "^0.543.0",
    "lucide-react-native": "^0.543.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-web": "^0.19.13",
    "recharts": "^3.2.0",
    "style-loader": "^3.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "@expo/metro-runtime": "~4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^20.0.2",
    "@types/react": "~18.3.12",
    "autoprefixer": "^10.4.21",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
}
